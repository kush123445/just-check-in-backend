{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js", 
        "use": "@vercel/node"
      },
      {
        "src": "app/build/**", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)", 
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)", 
        "dest": "app/build/$1"
      }
    ]
  }
  